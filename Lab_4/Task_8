#include <iostream>
#include <string>
#include<cmath>
using namespace std;

int main()
{
    int x,y;
    cout<<"Enter two numbers so I would compare them"<<"\n";
    cin>>x>>y;
    if (x==0 or y==0){
    cout<<"ERROR";
    }
    else{
    float a=1/x;
    float b=1/y;
    if (a=b){
        cout<<"Results are equal (by 0.000001 epsilon)";
        }
    else{
        cout<<"Results are not equal (by 0.000001 epsilon)";
        }
    }
}
