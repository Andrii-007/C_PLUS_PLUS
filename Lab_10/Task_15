#include <iostream>
#include <string>
#include<cmath>
#include <ctime>
#include <cstdlib>
using namespace std;

int main()
{
int m=3;
int n=3;
int i,j;
int d1=0;
int d2=0;
int arr[10][10]; 
srand(time(NULL));

for(i=0;i<m;i++){
    for(j=0;j<n;j++){
        arr[i][j]=rand()% 90+10;
        cout << arr[i][j] << "\t";
    }
    cout << endl;
}
for (int j = 0; j < n; j++) { 
        for (int i = 0; i < m; i++) {
            if (i==j) {
                d1+=arr[i][j];
            }
             if (i+j==m-1) {
               d2+=arr[i][j];
            }
        }
    }
cout << endl;
cout << "Sum of all numbers in diagonal 1 is:"<<d1<<endl;
cout << "Sum of all numbers in diagonal 2 is:"<<d2<<endl;


cout<<"---------------------------------------------------------------------------------------"<<endl;


}
