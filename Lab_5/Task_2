#include <iostream>
#include <string>
#include<cmath>
using namespace std;

int main() {
      int x;
      long int y;

    A1:for (int i = 1; i <= 10; i ++) { 
        cout << "x" << i << " = ";
        cin >> x;
        if (x>0){
        y += x*x;
        }
        else{
        cout << "ERROR"<<endl;
        y=0;
        goto A1;
            }
    }

    cout << "Summ of all didgets squares  = " << y << endl;

}
