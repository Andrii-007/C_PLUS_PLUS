#include <iostream>
#include <string>
#include<cmath>
using namespace std;

int main()
{
  int a[12] = {2,5,1,3,10,8,11,0,4,7,9,6};
  int x=0;
  int t;
  int i;
  int A = sizeof(a) / sizeof(a[0]);
    for (i = 0; i < A-1; i ++) {
        for ( x = 0; x < A - 1; x++){
        if (a[x]>a[x+1]){
            t=a[x];
            a[x]=a[x+1];
            a[x+1]=t;
            x++;
            }
        }
        
    }
    for (i = 0; i < A; i++) {
        cout << a[i] << " ";
    }



for (i = 0; i < A-1; i ++) {
        for ( x = 0; x < A-1; x++){
        if (a[x]<a[x+1]){
            t=a[x];
            a[x]=a[x+1];
            a[x+1]=t;
            x++;
            }
        }
        
    }
    cout<<endl;
    for (i = 0; i < A; i++) {
        cout << a[i] << " ";
    }

}
