#include <iostream>
#include <string>
#include<cmath>
#include <ctime>
#include <cstdlib>
using namespace std;

int main()
{
int vector[] = { 3, -5, 7, 10, -4, 14, 5, 2, -13 };
int n = sizeof(vector) / sizeof(vector[0]);
int *a=vector;
int min =*a;

for (int i = 1; i < n; i++) {
    if(*(a+i)<min){
    min=*(a+i);
    }    
}
cout<<min;

return 0;
}
